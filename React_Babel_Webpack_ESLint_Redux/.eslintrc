{
  "env": {
    "node": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "globals": {
    "React": true,
    "document",true
  },
  "plugins": [
    "react"
  ],
  "extends": "airbnb",
  "rules": {
    semi: [2, "never"],
    "comma-dangle": 0,
    "no-console": 0,
    "id-length": 0,
    "react/prop-types": 0
  }
}